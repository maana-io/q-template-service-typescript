{
  "name": "maana-my-service",
  "author": "Maana, Inc.",
  "license": "MIT",
  "version": "1.0.0",
  "description": "TypeScript Maana Q Knowledge Service Template",
  "main": "dist/main.js",
  "repository": "git@github.com:maana-io/q-template-service-node-ts-basic",
  "scripts": {
    "docker-build": "docker build -t maana-service .",
    "docker-run": "docker run -p 8050:8050 maana-service",
    "test": "jest",
    "build": "npm run generate && tsc",
    "start": "node ./dist/main.js",
    "startdev": "ts-node ./src/main.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate": "graphql-codegen --config codegen.yml",
    "generate-watch": "npm run generate -- --watch"
  },
  "dependencies": {
    "apollo-server": "^3.6.2",
    "dotenv": "^14.3.2",
    "graphql": "^16.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-resolvers": "2.4.3",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "core-js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/test/tests/.*\\.(ts|tsx|js)$"
  }
}
